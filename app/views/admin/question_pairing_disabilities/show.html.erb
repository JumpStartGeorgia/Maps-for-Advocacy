<%- model_class = QuestionPairingDisability -%>
<%- model_class_trans = QuestionPairingDisabilityTranslation -%>
<% title t '.title', :default => model_class.model_name.human %>


<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              admin_question_pairing_disabilities_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_admin_question_pairing_disability_path(@question_pairing_disability), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              admin_question_pairing_disability_path(@question_pairing_disability),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm")) },
              :class => 'btn btn-danger' %>
  <%= link_to t('helpers.links.admin'), admin_path, :class => 'btn' %>
</div>

<p><strong><%= model_class.human_attribute_name(:question_pairing_id) %>:</strong>
<%= @question_pairing_disability.question %></p>

<p><strong><%= model_class.human_attribute_name(:disability_id) %>:</strong>
<%= @question_pairing_disability.disability_name %></p>

<p><strong><%= model_class.human_attribute_name(:has_content) %>:</strong>
<%= @question_pairing_disability.has_content %></p>

<p><strong><%= model_class_trans.human_attribute_name(:content) %>:</strong></p>
<div class="tinymce_format">
  <%= simple_format_no_tags(@question_pairing_disability.content, {}, {sanitize: false}) %>
</div>



