<%- model_class = District -%>
<%- model_class_trans = DistrictTranslation -%>
<% title t '.title', :default => model_class.model_name.human %>


<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              admin_districts_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_admin_district_path(@district), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              admin_district_path(@district),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm")) },
              :class => 'btn btn-danger' %>
	<%= link_to t('helpers.links.admin'), admin_path, :class => 'btn' %>
</div>

<p><strong><%= model_class.human_attribute_name(:id) %>:</strong>
<%= @district.id %></p>


<p><strong><%= model_class_trans.human_attribute_name(:name) %>:</strong>
<%= @district.name %></p>

<p><strong><%= model_class.human_attribute_name(:json) %>:</strong>
<%= @district.json %></p>


