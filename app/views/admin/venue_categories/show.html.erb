<%- model_class = VenueCategory -%>
<%- model_class_trans = VenueCategoryTranslation -%>
<% title t '.title', :default => model_class.model_name.human %>


<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              admin_venue_categories_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_admin_venue_category_path(@venue_category), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              admin_venue_category_path(@venue_category),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm")) },
              :class => 'btn btn-danger' %>
	<%= link_to t('helpers.links.admin'), admin_path, :class => 'btn' %>
</div>

<p><strong><%= model_class_trans.human_attribute_name(:name) %>:</strong>
<%= @venue_category.name %></p>


